    "directory": "packages/anthropic"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@dysporium-sdk/core": "workspace:*",
    "@dysporium-sdk/provider": "workspace:*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
